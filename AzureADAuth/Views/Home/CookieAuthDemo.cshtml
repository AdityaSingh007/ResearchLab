
@{
    ViewData["Title"] = "CookieAuthDemo";
}

<h2>CookieAuthDemo</h2>

@if (User.Identity.IsAuthenticated)
{
    <table class="table table-sm">
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}

