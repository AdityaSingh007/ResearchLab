
@{
    ViewBag.Title = "Car";
    Layout = "~/Views/Shared/_DynamicLayout.cshtml";
}

<h2>Cars</h2>
<div class="row">
    <div class="col-md-6">
        <table id="carsTable" class="table">
            <thead>
                <tr>
                    <th scope="col">License</th>
                    <th scope="col">Make</th>
                    <th scope="col">Model</th>
                    <th scope="col">Miles</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
</div>
@section scripts{
    <script src="~/Scripts/MyScripts/Car.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            CarService.LoadCarsPage();
        });
    </script>
}

